@model IEnumerable<POS_Demo.DataModels.Units_Details>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Id)</th>
        <th>@Html.DisplayNameFor(model => model.SaleDate)</th>
        <th>@Html.DisplayNameFor(model => model.Developer.Name)</th>
        <th>@Html.DisplayNameFor(model => model.Sales_Person.Name)</th>
        <th>@Html.DisplayNameFor(model => model.Unit_NO)</th>
        <th>@Html.DisplayNameFor(model => model.Unit_Volume)</th>
        <th>@Html.DisplayNameFor(model => model.Deal_Slab)</th>
        <th>@Html.DisplayNameFor(model => model.Commission_Amount)</th>
        <th>@Html.DisplayNameFor(model => model.Commission_Deduction)</th>
        <th>@Html.DisplayNameFor(model => model.Total_Commission)</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Id)</td>
            <td>@Html.DisplayFor(modelItem => item.SaleDate)</td>
            <td>@Html.DisplayFor(modelItem => item.Developer.Name)</td>
            <td>@Html.DisplayFor(modelItem => item.Sales_Person.Name)</td>
            <td>@Html.DisplayFor(modelItem => item.Unit_NO)</td>
            <td>@Html.DisplayFor(modelItem => item.Unit_Volume)</td>
            <td>@Html.DisplayFor(modelItem => item.Deal_Slab)</td>
            <td>@Html.DisplayFor(modelItem => item.Commission_Amount)</td>
            <td>@Html.DisplayFor(modelItem => item.Commission_Deduction)</td>
            <td>@Html.DisplayFor(modelItem => item.Total_Commission)</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
